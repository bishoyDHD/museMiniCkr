void Det_LumiGEM::subtractCMode(int t,int i,cmodeinfo_apv &info)
{
  for (int k=0; k<250; k++)		      
    {
      if ((i==0)||(i==1)) {
	if (k<122) {
	  xdat[k]-=info.x_apv1;
	  ydat[k]-=info.y_apv1;
	};
	if (k>121) {
	  xdat[k]-=info.x_apv2;
	  ydat[k]-=info.y_apv2;
	};
      };
      if (i==2) {
	if (k<122) xdat[k]-=info.x_apv1;
	if (k>121) xdat[k]-=info.x_apv2;
	if (k<128) ydat[k]-=info.y_apv1;
	if (k>127) ydat[k]-=info.y_apv2;
      };
    };
}
 

