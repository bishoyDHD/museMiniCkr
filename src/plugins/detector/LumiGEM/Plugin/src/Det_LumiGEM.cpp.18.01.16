void Det_LumiGEM::InitHitFinder()
{
  hitfinder = new cHitFinder(250, 250);
  hitfinder->SetClusterSigmaRange(valCFSigmaMin, valCFSigmaMax);
  // allocate memory for processed and raw data (for one GEM at a time only):
  xdat    = new int[256];
  ydat    = new int[256];
  xdatraw = new int[256];
  ydatraw = new int[256];
}

